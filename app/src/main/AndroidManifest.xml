<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trial.wanx">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".TrialApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.page.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.page.activity.ImageLookActivity"
            android:theme="@style/ImageLookTheme" />
        <activity android:name=".ui.page.activity.NewsDetailActivity"/>
    </application>

</manifest>